function sessionAboutToEnd(){
	var oHead = document.getElementsByTagName('head')[0];
	loadScript(oHead, 'https://staticchk.tbe.taleo.net/chk01/ats/cacheable/R16-04-21-17-37-20-16.A2_prod/js/UIv8/engine.js',  function() {
