<script>
    var customUrl = '';
    var standardUrl = '/index.smpl?arg=jb_apply&POST_ID=3224017';

    
        
        var postURL = 'http://vitalsourcestaffing.force.com/jobboard/Jobregister?JobId=a0Fo000000KgWzpEAF&Jobboard=vitalsourcestaffing';
        
    
    
    var target = '';
    
    

    if (customUrl || postURL) {
        target = '_blank';
    }
    
    standardUrl = (postURL) ? postURL : standardUrl;
    
    var myUrl = (customUrl) ? customUrl : standardUrl;
    
    
    
    document.getElementById('lowerApply').href=myUrl;
    document.getElementById('upperApply').href=myUrl;
    
    document.getElementById('lowerApply').target=target;
    document.getElementById('upperApply').target=target;
    
    
    var send_reminder = function(formNum) {

        var postid = '3224017';
        var email = jQuery('#apply-later-email'+formNum).val();
        var submitButton = jQuery('#btn-apply-later'+formNum);

        if (email) {

            submitButton.button('loading');

            // need to send off an update to register the update with our systems
            
            jQuery.ajax({
                      url: "/ajax_rq.smpl",
                      global: false,
                      type: "POST",
                      data: {
                                fn: 'jb_send_reminder',
                                pid: 'gwt',
                                h: 'b4d6ab2071229cbbd121821c93c4dcef',
    							t: '1462960265',
                                email: email,
                                postid:postid
                      },
                      success: function(data,code,xhr){
                         // close up send later box & display success msg
                         submitButton.toggleClass('btn-primary, btn-success').button('success');
                         setTimeout(function() {
                            jQuery('.apply-later-collapse:visible').slideToggle('fast', function() {
                                submitButton.toggleClass('btn-primary, btn-success').button('reset');
                                jQuery('#apply-later-email'+formNum).val('');
                                jQuery('#apply-later-heading, #apply-later-lower-heading').addClass('collapsed');
                            });
                         }, 2000);
                      }
                   });
        }

    }    
    
</script> 
     





            
        
        
        
    
